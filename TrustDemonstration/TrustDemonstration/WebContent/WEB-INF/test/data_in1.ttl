	rrk:hasValue "High"^^xsd:string.
	
rrk:cyber_001 a rrk:Cyber, owl:NamedIndividual;
	rrk:hasValue "Low"^^xsd:string;
	rrk:hasQualityofInformation rrk:qualityofinformation_001;
	rrk:hasQualityofService rrk:qualityofservice_001.
##############	
